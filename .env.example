# IPMI Fan Control Environment Variables
# Copy this file to .env and customize the values for your environment

# === Connection Parameters ===
IPMI_HOST=192.168.1.100
IPMI_PORT=623
IPMI_USERNAME=admin
IPMI_PASSWORD=your_password_here
IPMI_INTERFACE=lanplus

# === General Settings ===
IPMI_DEBUG=false
IPMI_AUTO_RESTORE=true
IPMI_OUTPUT_FORMAT=table

# === PID Controller Parameters ===
IPMI_PID_TARGET_TEMP=60.0
IPMI_PID_INTERVAL=30.0
IPMI_PID_P_GAIN=0.1
IPMI_PID_I_GAIN=0.02
IPMI_PID_D_GAIN=0.01
IPMI_PID_MIN_SPEED=30.0
IPMI_PID_MAX_SPEED=100.0
IPMI_PID_RUNTIME=

# === Test Parameters ===
IPMI_TEST_QUICK=true
IPMI_TEST_DIAGNOSTIC=false